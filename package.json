{
  "type": "module",
  "bin": "./run.js",
  "name": "run",
  "scripts": {
    "purge": "rm -rf dist run run-*",
    "build": "npx rollup -c rollup.config.js && npx pkg dist/run.js -t node18-macos-arm64 --public",
    "execute": "open run"
  },
  "dependencies": {
    "@inquirer/prompts": "^1.2.3",
    "chalk": "^5.2.0",
    "inquirer": "^9.2.6",
    "puppeteer": "^18.2.1",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-recaptcha": "^3.6.8",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "rollup": "^3.23.0",
    "user-agents": "^1.0.1394"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.0.2",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-executable": "^1.6.3"
  },
  "pkg": {
    "scripts": "run.js",
    "targets": [
      "node18-macos-arm64"
    ]
  }
}
